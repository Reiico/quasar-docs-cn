{
  "type": "component",
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/tab-panels"
  },
  "props": {
    "name": {
      "type": "Any",
      "desc": "Panel name",
      "required": true,
      "examples": [
        "accounts",
        "firstPanel",
        ":name=\"1\""
      ],
      "category": "general",
      "desc_cn": "面板名称"
    },
    "disable": {
      "type": "Boolean",
      "desc": "Put component in disabled mode",
      "category": "state",
      "desc_cn": "将组件置于禁用模式"
    },
    "dark": {
      "type": "Boolean",
      "desc": "Notify the component that the background is a dark color",
      "category": "style",
      "desc_cn": "设置组件背景为深色"
    }
  },
  "slots": {
    "default": {
      "desc": "Default slot in the devland unslotted content of the component",
      "desc_cn": "组件的devland unslotted内容中的默认插槽"
    }
  }
}