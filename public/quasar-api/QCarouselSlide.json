{
  "type": "component",
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/carousel"
  },
  "props": {
    "name": {
      "type": "Any",
      "desc": "Slide name",
      "required": true,
      "examples": [
        "accounts",
        "firstPanel",
        ":name=\"1\"",
        "accounts",
        "firstSlide",
        ":name=\"1\""
      ],
      "category": "model",
      "desc_cn": "幻灯片名称"
    },
    "disable": {
      "type": "Boolean",
      "desc": "Put component in disabled mode",
      "category": "state",
      "desc_cn": "将组件置于禁用模式"
    },
    "img-src": {
      "type": "String",
      "desc": "URL pointing to a slide background image (use public folder)",
      "transformAssetUrls": true,
      "examples": [
        "(public folder) src=\"img/my-bg.png\"",
        "(assets folder) src=\"~assets/my-img.png\"",
        "(relative path format) :src=\"require('./my_img.jpg')\"",
        "(URL) src=\"https://picsum.photos/500/300\""
      ],
      "category": "model"
    }
  },
  "slots": {
    "default": {
      "desc": "Default slot in the devland unslotted content of the component",
      "desc_cn": "组件的默认插槽"
    }
  }
}