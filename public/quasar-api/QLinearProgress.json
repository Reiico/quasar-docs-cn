{
  "type": "component",
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/linear-progress"
  },
  "props": {
    "size": {
      "type": "String",
      "desc": "Size in CSS units, including unit name or standard size name (xs|sm|md|lg|xl)",
      "examples": [
        "16px",
        "2rem",
        "xs",
        "md"
      ],
      "category": "style",
      "desc_cn": "带有 CSS 单位的尺寸大小，包括单位的名称或标准大小名称（xs | sm | md | lg | xl）"
    },
    "value": {
      "type": "Number",
      "desc": "Progress value (0.0 < x < 1.0)",
      "default": 0,
      "category": "model",
      "required": false,
      "desc_cn": "进度值（0.0<x<1.0）"
    },
    "buffer": {
      "type": "Number",
      "desc": "Optional buffer value (0.0 < x < 1.0)",
      "category": "behavior",
      "desc_cn": "可选缓冲值（0.0<x<1.0）"
    },
    "color": {
      "type": "String",
      "desc": "Color name for component from the Quasar Color Palette",
      "examples": [
        "primary",
        "teal-10"
      ],
      "category": "style",
      "desc_cn": "组件的颜色，来自 Quasar 调色板的颜色名称"
    },
    "track-color": {
      "type": "String",
      "desc": "Color name for component's track from the Quasar Color Palette",
      "examples": [
        "primary",
        "teal-10"
      ],
      "category": "style",
      "desc_cn": "组件进展的来自Quasar调色板的颜色名称"
    },
    "dark": {
      "type": "Boolean",
      "desc": "Notify the component that the background is a dark color",
      "category": "style",
      "desc_cn": "通知组件背景为深色"
    },
    "reverse": {
      "type": "Boolean",
      "desc": "Reverse direction of progress",
      "category": "behavior",
      "desc_cn": "逆转进度方向"
    },
    "stripe": {
      "type": "Boolean",
      "desc": "Draw stripes; For determinate state only (for performance reasons)",
      "category": "content",
      "desc_cn": "画条纹；仅用于确定状态（出于性能原因）"
    },
    "indeterminate": {
      "type": "Boolean",
      "desc": "Put component into indeterminate mode",
      "category": "behavior",
      "desc_cn": "将组件置于不确定模式"
    },
    "query": {
      "type": "Boolean",
      "desc": "Put component into query mode",
      "category": "behavior",
      "desc_cn": "将组件置于查询模式"
    },
    "rounded": {
      "type": "Boolean",
      "desc": "Applies a small standard border-radius for a squared shape of the component",
      "category": "style",
      "desc_cn": "为组件的方形应用较小的标准border-radius"
    },
    "instant-feedback": {
      "type": "Boolean",
      "desc": "No transition when model changes",
      "category": "behavior",
      "desc_cn": "模型更改时无过渡动画"
    },
    "animation-speed": {
      "type": [
        "String",
        "Number"
      ],
      "desc": "Animation speed (in milliseconds, without unit)",
      "examples": [
        500,
        "1200"
      ],
      "category": "style",
      "default": 2100,
      "addedIn": "v2.3",
      "required": false
    }
  },
  "slots": {
    "default": {
      "desc": "Suggestion: QTooltip",
      "desc_cn": "建议：QTooltip"
    }
  }
}