{
  "type": "plugin",
  "meta": {
    "docsUrl": "https://v2.quasar.dev/quasar-plugins/addressbar-color"
  },
  "injection": "$q.addressbarColor",
  "methods": {
    "set": {
      "desc": "Sets addressbar color (for browsers that support it)",
      "params": {
        "hexColor": {
          "type": "String",
          "desc": "Color in hex format",
          "required": true,
          "examples": [
            "#ff0000"
          ],
          "desc_cn": "十六进制格式的颜色"
        }
      },
      "desc_cn": "设置地址栏颜色（对于支持它的浏览器）"
    }
  }
}