{
  "type": "component",
  "meta": {
    "docsUrl": "https://v2.quasar.dev/vue-components/carousel"
  },
  "props": {
    "position": {
      "type": "String",
      "desc": "Side/corner to stick to",
      "default": "bottom-right",
      "values": [
        "top-right",
        "top-left",
        "bottom-right",
        "bottom-left",
        "top",
        "right",
        "bottom",
        "left"
      ],
      "category": "position",
      "required": false,
      "desc_cn": "要粘贴的边/角"
    },
    "offset": {
      "type": "Array",
      "desc": "An array of two numbers to offset the component horizontally and vertically (in pixels)",
      "default": "[18, 18]",
      "examples": [
        "[8, 8]",
        "[5, 10]"
      ],
      "category": "position",
      "required": false,
      "desc_cn": "两个数字组成的数组，用于水平和垂直偏移组件（以像素为单位）"
    }
  },
  "slots": {
    "default": {
      "desc": "Default slot in the devland unslotted content of the component",
      "desc_cn": "组件的devland unslotted内容中的默认插槽"
    }
  }
}